@model ViljaladuMVC.Models.Koorma

@{
    ViewData["Title"] = "Lisa";
}

<h1>Lisa lahkumismass</h1>

<h4>Koorma</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="LisaLahkumismass">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <label asp-for="AutoNumber" class="control-label"></label>
                <input asp-for="AutoNumber" class="form-control" readonly/>
                <span asp-validation-for="AutoNumber" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="SisenemisMass" class="control-label"></label>
                <input asp-for="SisenemisMass" class="form-control" readonly/>
                <span asp-validation-for="SisenemisMass" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="LahkumisMass" class="control-label"></label>
                <input asp-for="LahkumisMass" class="form-control" />
                <span asp-validation-for="LahkumisMass" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Lisa" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Lahkumismassita">Tagasi</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
